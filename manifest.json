{
    "manifest_version": 3,
    "name": "GXCorner for All",
    "version": "1.0",
    "description": "Allows all browsers to open GXCorner",
    "short_description ": "Access GXCorner without Opera",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["https://gxcorner.games/*"],
        "world": "MAIN",
        "run_at": "document_end",
        "js": ["content.js"]
      }
    ]
  }
  