{
  "manifest_version": 3,
  "name": "GXCorner for All",
  "version": "1.0.5",
  "description": "Allows all browsers to open GXCorner",
  "short_description ": "Access GXCorner without Opera",
  "content_scripts": [
    {
      "matches": ["https://gxcorner.games/*"],
      "world": "MAIN",
      "run_at": "document_start",
      "js": ["content.js"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "gx-corner-for-all@ufukbakan"
    }
  }
}
  